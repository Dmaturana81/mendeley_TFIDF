journal = {Journal of Biological Chemistry},
keywords = {--------------------------------------------------,background,binds to the,cell surface,competition assay,factor xiia,fxiia,glycosaminoglycan,heparan sulfate,however,proteins,proteoglycan,pulmonary fibrosis,saccharides,underlying mechanism},
