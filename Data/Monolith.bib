journal = {Toxins},
keywords = {Biomolecular Interaction,Monolith NT.115,Protein-Protein,fusion proteins,peptides,proteins},
mendeley-tags = {fusion proteins,peptides,proteins},
number = {7},
pages = {2162--2176},
title = {{The Combined Repetitive Oligopeptides of Clostridium difficile Toxin A Counteract Premature Cleavage of the Glucosyl-Transferase Domain by Stabilizing Protein Conformation}},
