journal = {Differentiation},
keywords = {DNA demethylation,Gadd45,LC-MS/MS,LC–MS/MS,TET,hmC,proteins},
mendeley-tags = {proteins},
number = {1–3},
pages = {59--68},
pmid = {26546041},
publisher = {Elsevier},
title = {{GADD45a physically and functionally interacts with TET1}},
url = {http://www.sciencedirect.com/science/article/pii/S0301468115300505 http://dx.doi.org/10.1016/j.diff.2015.10.003},
