title = {{Identification and Isolation of Glucosytransferases (GT) Expressed Fungi Using a Two-Photon Ratiometric Fluorescent Probe Activated by GT}},
url = {https://pubs.acs.org/doi/10.1021/acs.analchem.8b02857},
volume = {90},
