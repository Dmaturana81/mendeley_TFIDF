keywords = {EMSA,MST,YFP,crystallography,fusion proteins,high impact journal,plant protein,protein-DNA,stemcells},
mendeley-tags = {EMSA,MST,YFP,crystallography,fusion proteins,high impact journal,plant protein,protein-DNA,stemcells},
number = {1},
pages = {1--16},
pmid = {32376862},
publisher = {Springer US},
title = {{Structural basis for the complex DNA binding behavior of the plant stem cell regulator WUSCHEL}},
