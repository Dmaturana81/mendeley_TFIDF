@article{Feng2018,
abstract = {As well known, fungi as an important biocatalysis model of glucosylation, were widely applied for bioactive compounds glucosylation mediated by the intracellular glucosytransferases (GTs). However, there is no efficient method for the real-time detection of GTs and the rapid isolation of the target fungi strains with the high expression of GTs. In the present work, we firstly developed a two-photon ratiometric fluorescent probe N-(n-butyl)-4-hydroxy-1,8-naphthalimide (NHN) for detecting the glucosyltransferases activity and intracellular imaging of GTs. Under the UV light (365 nm), the transformed product of NHN mediated by intracellular glucosyltransferase displayed blue emission to guide the rapid isolation of fungal strains possessing overexpression of GTs from the complex soil samples. Finally, by using the fluorescent probe, two target fungi were isolated and identified to be Rhizopus oryzae and Mucor circinelloides by molecular analysis, and they exhibited a robust capability to regioand stereospecific O-glycosylation. Our results fully demonstrated that NHN may be a promising tool for guiding real-time GTs activity in fungal strains, and even for developing natural fungal strains with GTs overexpression.},
author = {Feng, Lei and Li, Ping and Hou, Jie and Cui, Yong-Lei and Tian, Xiangge and Yu, Zhenlong and Cui, Jing-nan and Wang, Chao and Huo, Xiaokui and Ning, Jing and Ma, Xiaochi},
