journal = {Plant Direct},
keywords = {MST,NHS-RED,SPR,amylase,barley,enzyme stability,hordeum vulgare,oxidative stress,plant proteins,protein interaction,protein-protein,serpin,slide},
mendeley-tags = {MST,NHS-RED,SPR,plant proteins,protein-protein,slide},
