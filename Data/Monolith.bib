doi = {10.1002/open.201900167},
file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Zhang, Nagel‐Steger, Willbold - 2019 - Solution‐Based Determination of Dissociation Constants for the Binding of A$\beta$42 to Antibodies.pdf:pdf},
