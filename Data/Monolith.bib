keywords = {2Bind,MST,NHS-RED,NT.115pico,protein-peptide,receptor},
mendeley-tags = {2Bind,MST,NHS-RED,NT.115pico,protein-peptide,receptor},
number = {11},
pages = {3383--3391},
title = {{Computationally Designed Bispecific MD2/CD14 Binding Peptides Show TLR4 Agonist Activity}},
