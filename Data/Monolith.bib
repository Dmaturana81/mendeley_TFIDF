file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Dabrowski et al. - 2015 - Redox-Sensitive Structure and Function of the First Extracellular Loop of the Cell–Cell Contact Protein Clau.pdf:pdf},
isbn = {1523-0864},
issn = {1523-0864},
journal = {Antioxidants {\&} Redox Signaling},
