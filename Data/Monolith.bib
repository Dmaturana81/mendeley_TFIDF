issn = {2050084X},
journal = {eLife},
keywords = {SPR,detergents,enzymes,high affinity,membrane proteins,proteins},
mendeley-tags = {SPR,detergents,enzymes,high affinity,membrane proteins,proteins},
number = {JANUARY2016},
pages = {1--24},
