issn = {1345-711X},
journal = {Journal of Structural and Functional Genomics},
keywords = {Cladosporin,Inhibition,KRS,Malaria,X-ray crystal structure,enzymes,proteins,small molecules},
mendeley-tags = {enzymes,proteins,small molecules},
month = {jun},
number = {2},
pages = {63--71},
pmid = {24935905},
title = {{Structural basis of malaria parasite lysyl-tRNA synthetase inhibition by cladosporin}},
