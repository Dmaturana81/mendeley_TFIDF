journal = {New Phytologist},
keywords = {Bipolaris sorokiniana,CF594,Colletotrichum tofieldiae,MST,R-PE,carbohydrate-binding lectin,cell-wall-integrity-and-stress-response-component,glucan-matrix,isothermal titration calorimetry (ITC),microscale thermophoresis,plant proteins,protein-glucan,protocol},
mendeley-tags = {CF594,MST,R-PE,plant proteins,protein-glucan,protocol},
