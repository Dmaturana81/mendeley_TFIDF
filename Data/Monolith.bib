issn = {10898638},
journal = {Journal of Molecular Biology},
keywords = {SPR,chaperones,cryo-electron microscopy,macromolecular,molecular dynamics simulation,protein folding,proteins,ribosome-binding domain,ribosomes},
mendeley-tags = {SPR,macromolecular,proteins,ribosomes},
number = {18},
