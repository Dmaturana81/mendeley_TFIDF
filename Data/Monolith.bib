issn = {15524469},
journal = {Nature Chemical Biology},
keywords = {CRISPR,Cas9,EMSA,MST,Nobel prize winner,Tris-NTA,proten-RNA},
mendeley-tags = {CRISPR,Cas9,EMSA,MST,Nobel prize winner,Tris-NTA,proten-RNA},
