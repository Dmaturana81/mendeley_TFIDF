pages = {{\#}pagerange{\#}},
publisher = {Elsevier Ltd},
title = {{Acetylation of Response Regulator Proteins, TcrX and MtrA in M. tuberculosis Tunes their Phosphotransfer Ability and Modulates Two-Component Signaling Crosstalk}},
