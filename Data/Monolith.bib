issn = {00280836},
journal = {Nature},
keywords = {*Host Specificity,Animals,Biological,Birds/metabolism/virology,Chick Embryo,Crystallography,Ferrets/*virology,H5N1 Subtype/chemistry/*genetic,Hemagglutinin Glycoproteins,Humans,Influenza A Virus,Influenza Virus/*chem,Models,Molecular,Monolith NT.115,Mutation,Orthomyxoviridae Infections/*transmission/*virolog,Protein Conformation,Protein-Protein,Receptors,Species Specificity,Virus/*metabolism,X-Ray,high impact journal,low affinity,membrane proteins,proteins,saccharides},
