number = {12},
pages = {2101--2106},
pmid = {24815687},
publisher = {Federation of European Biochemical Societies},
title = {{Direct and selective small-molecule inhibition of photosynthetic PEP carboxylase: New approach to combat C4 weeds in arable crops}},
