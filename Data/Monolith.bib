isbn = {1469-896X},
issn = {09618368},
journal = {Protein Science},
keywords = {ABC transporter,Borrelia burgdorferi,Ligand-binding protein,Lipoprotein,Phosphate-binding protein,X-ray crystallography,ions,proteins},
mendeley-tags = {ions,proteins},
month = {feb},
number = {2},
pages = {200--212},
pmid = {24318969},
title = {{Sequence, biophysical, and structural analyses of the PstS lipoprotein (BB0215) from Borrelia burgdorferi reveal a likely binding component of an ABC-type phosphate transporter}},
