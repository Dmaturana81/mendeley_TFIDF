}
@article{Pinker2017,
abstract = {RNase P is a universal enzyme that removes 5' leader sequences from tRNA precursors. The enzyme is therefore essential for maturation of functional tRNAs and mRNA translation. RNase P represents a unique example of an enzyme that can occur either as ribonucleoprotein or as protein alone. The latter form of the enzyme, called protein-only RNase P (PRORP), is widespread in eukaryotes in which it can provide organellar or nuclear RNase P activities. Here, we have focused on Arabidopsis nuclear PRORP2 and its interaction with tRNA substrates. Affinity measurements helped assess the respective importance of individual pentatricopeptide repeat motifs in PRORP2 for RNA binding. We characterized the PRORP2 structure by X-ray crystallography and by small-angle X-ray scattering in solution as well as that of its complex with a tRNA precursor by small-angle X-ray scattering. Of note, our study reports the first structural data of a PRORP-tRNA complex. Combined with complementary biochemical and biophysical analyses, our structural data suggest that PRORP2 undergoes conformational changes to accommodate its substrate. In particular, the catalytic domain and the RNA-binding domain can move around a central hinge. Altogether, this work provides a refined model of the PRORP-tRNA complex that illustrates how protein-only RNase P enzymes specifically bind tRNA and highlights the contribution of protein dynamics to achieve this specific interaction.},
author = {Pinker, Franziska and Schelcher, C{\'{e}}dric and Fern{\'{a}}ndez-Mill{\'{a}}n, Pablo and Gobert, Anthony and Birck, Catherine and Thureau, Aur{\'{e}}lien and Roblin, Pierre and Gieg{\'{e}}, Philippe and Sauter, Claude and Fernandez-Millan, Pablo and Gobert, Anthony and Birck, Catherine and Thureau, Aur{\'{e}}lien and Roblin, Pierre and Gieg{\'{e}}, Philippe and Sauter, X. Claude},
doi = {10.1074/jbc.M117.782078},
file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Pinker et al. - 2017 - Biophysical analysis of Arabidopsis protein-only RNase P alone and in complex with tRNA provides a refined mo(10).pdf:pdf},
