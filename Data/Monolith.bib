@article{Ding2019b,
abstract = {To date, few structural models of VHH antibody binding to low molecular weight haptens have been reported. Here, we report the crystal structure of cortisol binding to its VHH antibody NbCor at pH 3.5 and 10.5. Cortisol binds to NbCor mainly by burying itself under the tunnel formed by the complementarity determining region 1 (CDR1) of NbCor. The affinity of NbCor binding to cortisol and similar compounds was also verified by a microscale thermophoresis assay. Combining our findings with several previously reported structures of hapten-VHH antibody complexes, we propose that VHH antibodies exhibit a special mechanism of binding small haptens by encapsulating them in a tunnel formed by CDR1. Our findings provide useful structural information for the further development and optimization of hapten-specific VHH antibodies.},
author = {Ding, Lulu and Wang, Ziying and Zhong, Peiyu and Jiang, He and Zhao, Zhixuan and Zhang, Yiran and Ren, Zhen},
