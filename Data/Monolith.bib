file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Mao et al. - 2017 - New peptide inhibitors modulate the self-assembly of islet amyloid polypeptide residues 11–20 in vitro(3).pdf:pdf},
issn = {18790712},
journal = {European Journal of Pharmacology},
keywords = {Amyloid formation,Islet amyloid polypeptide,MST only,Microscale thermophoresis,Molecular interaction,Peptide inhibitors,aggregation,dimerization {\&} oligomerization,peptides,thermodynamics},
mendeley-tags = {MST only,aggregation,dimerization {\&} oligomerization,peptides,thermodynamics},
number = {September 2016},
pages = {102--110},
publisher = {Elsevier B.V.},
title = {{New peptide inhibitors modulate the self-assembly of islet amyloid polypeptide residues 11–20 in vitro}},
