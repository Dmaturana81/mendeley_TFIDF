issn = {14602431},
journal = {Journal of experimental botany},
keywords = {Affinity purification,Arabidopsis thaliana,NDPK,glutathionylation,ligand induced thermal shift,metabolites,nanoDSF,plant,plant proteins,protein-small molecule,protein–metabolite interactions,protein–protein interactions},
mendeley-tags = {ligand induced thermal shift,nanoDSF,plant proteins,protein-small molecule},
