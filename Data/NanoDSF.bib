issn = {18733441},
journal = {European Journal of Pharmaceutics and Biopharmaceutics},
keywords = {Aggregation,Biopharmaceutical proteins,DLS,Excipients,Monoclonal antibodies,NMR,Protein stability,Protein-excipient interactions,SEC,aggregation,excipients,formulation,nanoDSF,therapeutic proteins},
