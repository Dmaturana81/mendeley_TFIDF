issn = {18733476},
journal = {International Journal of Pharmaceutics},
keywords = {Artificial neural network,Biopharmaceutics,DLS,Drug development,Machine learning,Protein aggregation,Protein formulation,Protein stability,SECMALS,nanoDSF,storage condition,therapeutic proteins},
mendeley-tags = {DLS,SECMALS,nanoDSF,storage condition,therapeutic proteins},
pages = {119039},
publisher = {Elsevier B.V.},
title = {{Application of machine learning to predict monomer retention of therapeutic proteins after long term storage}},
