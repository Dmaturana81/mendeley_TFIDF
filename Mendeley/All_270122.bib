keywords = {CAUTION,Cyclic peptide,Drug discovery,Fluorescence polarization (FP),Hippo signaling pathway,Ki,Protein–protein interaction (PPI),Small molecule inhibitor,TEAD,Tycho,YAP,buffer screen,pH effect,wrong ramp rate,$\Omega$-loop},
