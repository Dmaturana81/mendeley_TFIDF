pages = {2241--2250},
publisher = {Elsevier B.V.},
title = {{Novel microscale approaches for easy, rapid determination of protein stability in academic and commercial settings}},
url = {http://dx.doi.org/10.1016/j.bbapap.2014.09.016 http://linkinghub.elsevier.com/retrieve/pii/S1570963914002519},
volume = {1844},
year = {2014}
}
