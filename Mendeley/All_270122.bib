keywords = {CCR2 antagonist,Endothelium,Monocyte,SAW,Target-sensitive liposomes,Targeted drug delivery,VCAM-1},
mendeley-tags = {SAW},
pages = {18--29},
pmid = {25438248},
publisher = {Elsevier B.V.},
title = {{VCAM-1 directed target-sensitive liposomes carrying CCR2 antagonists bind to activated endothelium and reduce adhesion and transmigration of monocytes}},
url = {http://dx.doi.org/10.1016/j.ejpb.2014.11.016},
volume = {89},
year = {2015}
}
@article{Eliahoo2015,
abstract = {The single-stranded DNA (ssDNA)/RNA binding protein translin was suggested to be involved in chromosomal translocations, telomere metabolism, and mRNA transport and translation. Oligonucleotide binding surfaces map within a closed cavity of translin octameric barrels, raising the question as to how DNA/RNA gain access to this inner cavity, particularly given that, to date, none of the barrel structures reported hint to an entryway. Here, we argue against a mechanism by which translin octamers may "dissociate and reassemble" upon RNA binding and report a novel "open"-barrel structure of human translin revealing a feasible DNA/RNA entryway into the cavity. Additionally, we report that translin not only is confined to binding of ssDNA oligonucleotides, or single-stranded extensions of double-stranded DNA (dsDNA), but also can bind single-stranded sequences internally embedded in dsDNA molecules.},
author = {Eliahoo, Elad and Marx, Ailie and Manor, Haim and Alian, Akram},
doi = {10.1016/j.jmb.2014.11.013},
file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Eliahoo et al. - 2015 - A novel open-barrel structure of octameric translin reveals a potential RNA entryway(3).pdf:pdf},
isbn = {0022-2836},
issn = {10898638},
journal = {Journal of Molecular Biology},
