journal = {Bioscience Reports},
keywords = {MST,NTA labelling,Tycho,chrystallography,enzymes,nanoDSF,pH effect},
mendeley-tags = {MST,NTA labelling,Tycho,chrystallography,enzymes,nanoDSF,pH effect},
