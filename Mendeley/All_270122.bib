isbn = {1203801998},
journal = {J Bioscience},
keywords = {CD,abbreviations,abelmoschus esculentus,acetonitrile,acn,baee,cd,cid,circular dichroism,collision induced dissociation,differential scanning fluorimetry,dithiothreitol,dsf,dtt,kunitz protease inhibitor,l -arginine ethyl ester,malvaceae,n - a -benzoyl-,nano-esi mass spectrometry,nanoDSF,okra protease inhibitor,opi,pi,plant proteins,protease,rp-hplc},
