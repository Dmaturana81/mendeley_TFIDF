file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Kunz et al. - 2019 - Nanobody stability engineering by employing the $\Delta$Tm shift a comparison with apparent rate constants of heat-induce.pdf:pdf},
issn = {1741-0126},
journal = {Protein Engineering, Design, and Selection},
keywords = {aggregation,disul fi de bond,nanoDSF,nanobodies,nanobody,protein aggregation,single-domain antibody,stability engineering},
mendeley-tags = {aggregation,nanoDSF,nanobodies},
