issn = {10916490},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
keywords = {CAUTION,Crystal structure,Inhibitor,Metal binding,Protein phosphatase 1 (PP1),SDS22,Tm and not Ti,Tycho,crystallography,enzymes,mutants},
mendeley-tags = {CAUTION,Tm and not Ti,Tycho,crystallography,enzymes,mutants},
