journal = {Journal of Biological Chemistry},
keywords = {-sensor,5-bisphosphate,Alexa-488,LabelFree,MST,ND,a ca 2,background,interacts with the anionic,mutants,of neuronal exocytosis,phosphatidylinositol 4,protein-ion,slide,synaptotagmin,synaptotagmin-1,thermophoresis},
