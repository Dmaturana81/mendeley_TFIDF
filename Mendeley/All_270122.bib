issn = {1362-4962},
journal = {Nucleic Acids Research},
keywords = {EMSA,FAM,ITC,MST,different methods-same affinity,protein-DNA,protein-RNA},
mendeley-tags = {EMSA,FAM,ITC,MST,different methods-same affinity,protein-DNA,protein-RNA},
pages = {1--13},
