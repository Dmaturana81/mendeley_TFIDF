@article{Ioannidis2015,
abstract = {Light-responsive proteins have been used in the field of optogenetics to control cellular functions. However, surprisingly, analogous approaches to regulate and alter the functions of RNA molecules by light remain underdeveloped. RNA aptamers and RNA devices can perform diverse intracellular functions and are important tools in synthetic biology. This report explores the challenges of and potential strategies for engineering light regulation into functional RNAs in cells. We discuss approaches for using existing light-regulated proteins and small molecules to control RNA function in living cells. In addition, applications of light-regulated RNAs for synthetic biology and for studying functions of endogenously expressed RNAs are discussed.},
