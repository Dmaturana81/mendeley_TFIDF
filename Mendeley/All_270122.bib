journal = {Nature Communications},
keywords = {MST,NT.115pico,Star635P,antibody,protein-protein,protocol,slide,stoichiometry},
mendeley-tags = {MST,NT.115pico,Star635P,antibody,protein-protein,protocol,slide,stoichiometry},
