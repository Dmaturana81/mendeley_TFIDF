isbn = {0022-2860},
journal = {Journal of Molecular Structure},
keywords = {Binding studies,Biomolecular Interaction,Enzyme Kinetics,High affinity Interactions,ITC,Interaction analysis,Microscale Thermophoresis,Monolith NT.115,Monolith NT.115Pico,Monolith NT.LabelFree,Nucleic Acids-Interaction,Protein folding,Protein-Protein,Purification-free,SPR,Stoichiometry,Thermodynamics,Unfolding,nucleic acids DNA,proteins,reviews,thermodynamics},
