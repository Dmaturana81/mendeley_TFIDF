@article{Edwards2020,
abstract = {The impact of COVID-19 and the urgency to develop a vaccine against the SARS-CoV-2 virus cannot be overstated. The viral fusion spike (S) protein ectodomain is the primary target for vaccine development. Here we report an unexpected cold sensitivity of a stabilized SARS-CoV- 2 ectodomain construct currently being widely used for immunogen design. We found that when stored at 22 or 37 °C for 1 week, the S-protein displayed well-ordered trimeric spikes by negative stain electron microscopy. However, storage at 4 °C reduced the trimeric spikes to <10%, accompanied by decreased stability and enhanced exposure of the ACE-2 receptor binding site. Well-formed S particles could be recovered from cold-stored samples by a brief incubation at 37 °C. Our results will have broad impact on structural, functional and vaccine studies using the SARS-CoV-2 S ectodomain.},
author = {Edwards, Robert J and Mansouri, Katayoun and Stalls, Victoria and Manne, Kartik and Watts, Brian and Parks, Rob and Gobeil, Sophie M C and Janowska, Katarzyna and Li, Dapeng and Lu, Xiaozhi and Deyton, Margaret and Sprenz, Jordan and Williams, Wilton and Saunders, Kevin and Gregory, D},
