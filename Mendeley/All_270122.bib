isbn = {4159801824196},
issn = {20452322},
journal = {Scientific Reports},
keywords = {aggregation,concentration dependence,nanoDSF,nanobodies},
mendeley-tags = {aggregation,concentration dependence,nanoDSF,nanobodies},
number = {1},
pages = {1--10},
title = {{The structural basis of nanobody unfolding reversibility and thermoresistance}},
volume = {8},
year = {2018}
