}
@article{Pradeep2015,
abstract = {Summary While recombinant human erythropoietin (rhEpo) has been widely used to treat anemia in cancer patients, concerns about its adverse effects on patient survival have emerged. A lack of correlation between expression of the canonical EpoR and rhEpo's effects on cancer cells prompted us to consider the existence of an alternative Epo receptor. Here, we identified EphB4 as an Epo receptor that triggers downstream signaling via STAT3 and promotes rhEpo-induced tumor growth and progression. In human ovarian and breast cancer samples, expression of EphB4 rather than the canonical EpoR correlated with decreased disease-specific survival in rhEpo-treated patients. These results identify EphB4 as a critical mediator of erythropoietin-induced tumor progression and further provide clinically significant dimension to the biology of erythropoietin.},
