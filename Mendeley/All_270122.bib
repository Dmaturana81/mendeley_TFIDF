isbn = {0039-9140},
journal = {Talanta},
keywords = {Amyloid formation,Islet amyloid polypeptide,Microscale thermophoresis,Molecular interaction,Solvent effect,dimerization & oligomerization,peptides,thermodynamics},
mendeley-tags = {dimerization & oligomerization,peptides,thermodynamics},
pages = {380--386},
title = {{New insights into side effect of solvents on the aggregation of human islet amyloid polypeptide 11–20}},
