mendeley-tags = {MST,NT.Labelfree,high impact journal,peptides,phages,proteins},
pages = {1--18},
pmid = {28099413},
publisher = {Nature Publishing Group},
title = {{Communication between viruses guides lysis–lysogeny decisions}},
