year = {2016}
}
@article{Pross2020,
abstract = {During their synthesis the C-tailed membrane proteins expose the membrane-spanning segment late from the ribosome and consequently can insert into the membrane only post-translationally. However, the C-tailed T6SS component SciP uses the bacterial SRP system for membrane targeting which operates co-translationally. Analysis of possible sequence regions in the amino-terminal part of the protein revealed two candidates that were then tested whether they function as SRP signal peptides. Both sequences were tested positive as synthetic peptides for binding to SRP. In addition, purified ribosomes with stalled nascent chains exposing either sequence were capable to bind to SRP and to SRP-FtsY complexes with high affinity. Together, the data suggest that both peptides can serve as SRP signal sequence promoting an early membrane targeting of SciP during its synthesis. Like observed for multi-spanning membrane proteins, the two cytoplasmic SRP signal sequences of SciP may also facilitate a re-targeting event, making the targeting more efficient. Importance C-tail proteins are anchored in the inner membrane with a transmembrane segment at the C-terminus in a N-in/C-out topology. Due to this topology the membrane insertion occurs only post-translationally. Nevertheless, the C-tail anchored protein SciP is targeted co-translationally by SRP. We report here that two cytoplasmic hydrophobic stretches in SciP are individually recognized by SRP and target the nascent protein to FtsY. The presence of two signal sequences may enable a re-targeting mechanism as already observed for multi-spanning membrane proteins, to make the post-translational insertion of SciP by YidC more efficient.},
author = {Pross, Eva and Kuhn, Andreas},
doi = {10.1128/jb.00312-20},
file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Pross, Kuhn - 2020 - Two SRP signal sequences are present in the amino-terminal domain of the C-tailed protein SciP.pdf:pdf},
issn = {0021-9193},
journal = {Journal of Bacteriology},
keywords = {MST,RED-NHS,bacteria,membrane proteins,mutants,ribosome-protein,ribosomes},
mendeley-tags = {MST,RED-NHS,bacteria,membrane proteins,mutants,ribosome-protein,ribosomes},
number = {October},
title = {{Two SRP signal sequences are present in the amino-terminal domain of the C-tailed protein SciP}},
year = {2020}
}
@article{Wild2019,
abstract = {Co-translational protein targeting to membranes depends on the regulated interaction of two ribonucleoprotein particles (RNPs): the ribosome and the signal recognition particle (SRP). Human SRP is composed of an SRP RNA and six proteins with the SRP GTPase SRP54 forming the targeting complex with the heterodimeric SRP receptor (SR) at the endoplasmic reticulum membrane. While detailed structural and functional data are available especially for the bacterial homologs, the analysis of human SRP was impeded by the unavailability of recombinant SRP. Here, we describe the large-scale production of all human SRP components and the reconstitution of homogeneous SRP and SR complexes. Binding to human ribosomes is determined by microscale thermophoresis for individual components, assembly intermediates and entire SRP, and binding affinities are correlated with structural information available for all ribosomal contacts. We show that SRP RNA does not bind to the ribosome, while SRP binds with nanomolar affinity involving a two-step mechanism of the key-player SRP54. Ultrasensitive binding of SRP68/72 indicates avidity by multiple binding sites that are dominated by the C-terminus of SRP72. Our data extend the experimental basis to understand the mechanistic principles of co-translational targeting in mammals and may guide analyses of complex RNP–RNP interactions in general.},
author = {Wild, Klemens and Juaire, Keven D and Soni, Komal and Shanmuganathan, Vivekanandan and Hendricks, Astrid and Segnitz, Bernd and Beckmann, Roland and Sinning, Irmgard},
doi = {10.1093/nar/gky1324},
file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Wild et al. - 2019 - Reconstitution of the human SRP system and quantitative and systematic analysis of its ribosome interactions.pdf:pdf},
journal = {Nucleic Acids Research},
keywords = {Atto647,MST,ribosome-protein,ribosomes,whole cells},
mendeley-tags = {Atto647,MST,ribosome-protein,ribosomes,whole cells},
pages = {1--13},
publisher = {Oxford University Press},
title = {{Reconstitution of the human SRP system and quantitative and systematic analysis of its ribosome interactions}},
year = {2019}
}
