publisher = {Elsevier Ltd},
title = {{One Target - Two Different Binding Modes: Structural Insights into Gevokizumab and Canakinumab Interactions to Interleukin-1??}},
url = {http://dx.doi.org/10.1016/j.jmb.2012.09.021},
volume = {425},
