issn = {09651748},
journal = {Insect Biochemistry and Molecular Biology},
keywords = {ITC,MST,NHS-RED,phlebotomus orientalis,phlebotomus perniciosus,protein-small molecule,yellow-related protein},
mendeley-tags = {ITC,MST,NHS-RED,protein-small molecule},
