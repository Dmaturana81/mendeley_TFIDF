issn = {15206017},
journal = {Journal of Pharmaceutical Sciences},
keywords = {AAV,AAV vector,adeno-associated virus,capsid,capsid thermal stability,gene therapy,high-throughput technology,intrinsic fluorescence,melting temperature,nanoDSF,vector},
