journal = {Acta Crystallographica Section D: Structural Biology},
keywords = {FITC,MST,SAXS,X-ray crystallography,crystallography,ectodomain,heparin,heparin binding,leucocyte common antigen-related protein,protein tyrosine phosphatase receptor,protein-GAG,protocol,slide,synaptogenesis,webinar},
