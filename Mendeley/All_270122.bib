issn = {2046-2441 (Electronic)},
journal = {Open biology},
keywords = {adhesin,binding protein,dna-binding protein,enzymes,heparin-binding protein,high affinity,leucine aminopeptidase,moonlighting protein,plasminogen-,proteins,saccharides},
mendeley-tags = {enzymes,high affinity,proteins,saccharides},
number = {1},
pages = {140175},
pmid = {25589579},
title = {{MHJ_0461 is a multifunctional leucine aminopeptidase on the surface of Mycoplasma hyopneumoniae.}},
