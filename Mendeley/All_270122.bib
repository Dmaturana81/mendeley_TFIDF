issn = {0021-9258},
journal = {Journal of Biological Chemistry},
keywords = {MST,NTA labelling,__________________________________________________,act,allostery,arabidopsis,autophosphorylation,binding protein,chloroplast development,domain,kinase signaling,kinases,ligand,plant proteins,protein import,protein kinase,protein phosphorylation,protein-metabolite,protein-protein},
mendeley-tags = {MST,NTA labelling,kinases,plant proteins,protein-metabolite,protein-protein},
pages = {jbc.RA119.010298},
pmid = {31594863},
title = {{The ACT domain in chloroplast precursor–phosphorylating STY kinases binds metabolites and allosterically regulates kinase activity}},
