journal = {Cancers},
keywords = {Alanine scan,CooP,FABP3,Glioblastoma,MST,Phage display,RED-NHS 2nd gen,SPR,cancer,protein-peptide},
mendeley-tags = {MST,RED-NHS 2nd gen,SPR,cancer,protein-peptide},
