@article{Kedrov2014,
abstract = {Molecular mechanisms of membrane protein targeting and folding have been extensively investigated over the last decade. Ribosome targeting to the membrane translocon represents an early stage of membrane protein biogenesis and is facilitated by the signal recognition particle (SRP). Here, we establish MicroScale Thermophoresis to analyze interactions between bacterial SRP and translating ribosomes. Binding affinity measured for wild-type SRP was at low nano-molar level, in agreement with previously published data. Modifications in the ribosome and SRP, however, altered the binding affinity. We demonstrate that assembly of the ribosome:SRP complex can be monitored using fluorescently-labeled ribosomes, which opens prospects for screening molecular determinants for this universal interaction.},
author = {Kedrov, Alexej and der Sluis, Van and Beckmann, Roland},
