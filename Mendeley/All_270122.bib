journal = {Biochimica et Biophysica Acta - Biomembranes},
keywords = {Cy5,Detergent-free,Dopamine receptor,GPCR,Lipodisq,MST,Microscale thermophoresis,Native Nanodiscs,Neurotensin,SMA polymer,SMALP,labeled peptide,membrane proteins,nanodiscs,protein-peptide},
