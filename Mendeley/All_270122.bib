isbn = {9788578110796},
issn = {1098-6596},
journal = {Application Note NT-PR-013},
keywords = {icle},
number = {9},
pages = {1689--1699},
pmid = {25246403},
title = {{Formulation Development}},
volume = {53},
year = {2013}
}
