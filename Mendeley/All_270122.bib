issn = {18790003},
journal = {International Journal of Biological Macromolecules},
keywords = {Arg90,CAUTION-Ka instead of Kd,Enantiomeric $\alpha$-aminophosphonate derivatives,ITC,Interactions,MST,TMV CP,mutants,protein-small molecule},
mendeley-tags = {CAUTION-Ka instead of Kd,ITC,MST,mutants,protein-small molecule},
pages = {603--610},
