file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Liberelle et al. - 2019 - MUC4-ErbB2 Oncogenic Complex Binding studies using Microscale Thermophoresis.pdf:pdf},
issn = {20452322},
journal = {Scientific Reports},
keywords = {CAUTION: 80 % MST power,GFP,MST only,Tris-NTA,biphasic curves,cancer,cell lysate,glycoprotein,protein-protein},
mendeley-tags = {CAUTION: 80 % MST power,GFP,MST only,Tris-NTA,biphasic curves,cancer,cell lysate,glycoprotein,protein-protein},
number = {1},
