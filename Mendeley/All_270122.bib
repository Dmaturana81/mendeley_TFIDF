journal = {Chemistry and Biology},
number = {12},
pages = {1618--1628},
pmid = {25455859},
publisher = {Elsevier Ltd},
title = {{Rational Design of Small Molecule Inhibitors Targeting the Ras GEF, SOS1}},
url = {http://dx.doi.org/10.1016/j.chembiol.2014.09.018},
volume = {21},
year = {2014}
}
