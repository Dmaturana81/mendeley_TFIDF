journal = {RNA Biol},
keywords = {Biomolecular Interaction,Monolith NT.115,Protein-Nucleic Acids,nucleic acids DNA,nucleic acids RNA,peptides,proteins},
mendeley-tags = {nucleic acids DNA,nucleic acids RNA,peptides,proteins},
number = {8},
