file = {:Volumes/Users/matu/Library/Application Support/Mendeley Desktop/Downloaded/Lauster et al. - 2017 - Antiviral Agents Hot Paper Multivalent Peptide – Nanoparticle Conjugates for Influenza-Virus Inhibition.pdf:pdf},
keywords = {MST,customer protocol,customer slide,influenza,nanoparticles,peptides,virus particles},
mendeley-tags = {MST,customer protocol,customer slide,influenza,nanoparticles,peptides,virus particles},
