journal = {Biosensors},
keywords = {Aptamer,Cy5,DNA-DNA,FITC,MST,ND,Protein-DNA,aptamers,biosensor,diagnostic,microarray,protein detection,targetinduced dissociation (TID),thermophoresis,vascular endothelial growth factor (VEGF)},
mendeley-tags = {Cy5,DNA-DNA,FITC,MST,ND,Protein-DNA,aptamers,diagnostic},
