issn = {00222836},
journal = {Journal of Molecular Biology},
keywords = {NMR,NMR epitope mapping,SPR,antibodies,binding mechanism,canakinumab and gevokizumab,competition assay,high affinity,interleukin-1??,proteins,receptor signaling,therapeutic antibodies},
mendeley-tags = {NMR,SPR,antibodies,binding mechanism,competition assay,high affinity,proteins},
number = {1},
