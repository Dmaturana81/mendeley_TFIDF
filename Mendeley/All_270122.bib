issn = {01681656},
journal = {Journal of Biotechnology},
keywords = {Biocatalysis,Bioconversion,Carboxylic acid,Carboxylic acid reductase,Thermofluor,Thermothelomyces thermophila,Vanillin,buffer screen,carboxylic acid reductase,enzymes,food,nanoDSF,protein,slide},
mendeley-tags = {Thermofluor,buffer screen,enzymes,food,nanoDSF,protein,slide},
number = {July},
pages = {44--51},
