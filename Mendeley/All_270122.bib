journal = {Biochemistry},
keywords = {DNA-ion,FAM,Journal Club,MST only,biphasic curves,cancer,mutants,thermodynamics},
mendeley-tags = {DNA-ion,FAM,Journal Club,MST only,biphasic curves,cancer,mutants,thermodynamics},
number = {38},
pages = {3955--3959},
pmid = {31468963},
publisher = {American Chemical Society},
title = {{Studying the Potassium-Induced G-Quadruplex DNA Folding Process Using Microscale Thermophoresis}},
