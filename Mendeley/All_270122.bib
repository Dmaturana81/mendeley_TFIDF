issn = {2041-1723},
journal = {Nature Communications},
keywords = {enzymes,high impact journal,ligand induced thermal shift,nanoDSF},
mendeley-tags = {enzymes,high impact journal,ligand induced thermal shift,nanoDSF},
number = {1},
