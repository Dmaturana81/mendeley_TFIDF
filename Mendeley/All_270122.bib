issn = {1083351X},
journal = {Journal of Biological Chemistry},
keywords = {CdaA:GlmM complex,MST,PPI,RED NHS 2nd gen dye},
mendeley-tags = {CdaA:GlmM complex,MST,PPI,RED NHS 2nd gen dye},
month = {nov},
