issn = {15204804},
journal = {Journal of Medicinal Chemistry},
keywords = {MST,RED NHS,VEGF vs peptides},
mendeley-tags = {MST,RED NHS,VEGF vs peptides},
month = {aug},
number = {15},
pages = {10900--10907},
