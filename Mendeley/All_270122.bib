@article{Breitsprecher2003,
abstract = {MircoScale Thermophoresis (MST) not only allows for a precise determination of binding constants, but can also be used to derive additional information about the molecular mechanism of the investigated interaction. For instance, MST can be used to discriminate between different binding modes, and can also be used to determine interaction stoichiometries. We illustrate the different information that can be derived from MST experiments on the example of the interaction of short Oligo(dT) nucleotides with single-strand binding protein from E. coli (EcoSSB). We show that MST can be used to easily and quickly determine the pM affinity, the binding mode and the stoichiometry of this interaction},
