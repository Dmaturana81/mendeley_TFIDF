title = {{Redox-Sensitive Structure and Function of the First Extracellular Loop of the Cell–Cell Contact Protein Claudin-1: Lessons from Molecular Structure to Animals}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84919333080&partnerID=tZOtx3y1 http://online.liebertpub.com/doi/abs/10.1089/ars.2013.5706},
