author = {Takacs, Maria and Petoukhov, Maxim V and Atkinson, R Andrew and Roblin, Pierre and Ogi, Fran{\c{c}}ois-Xavier and Demeler, Borries and Potier, Noelle and Chebaro, Yassmine and Dejaegere, Annick and Svergun, Dmitri I and Moras, Dino and Billas, Isabelle M L},
