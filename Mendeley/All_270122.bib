journal = {Protein Journal},
keywords = {CD,Chickpea,Circular dichroism spectroscopy,Differential scanning fluorimetry,Functional stability,Kunitz trypsin inhibitor,chemical denaturation,enzyme,nanoDSF,organic solvents,pH effect,plant proteins},
mendeley-tags = {CD,chemical denaturation,enzyme,nanoDSF,organic solvents,pH effect,plant proteins},
number = {0123456789},
publisher = {Springer US},
title = {{Functional Stability and Structural Transitions of a Kunitz trypsin Inhibitor from Chickpea (CaTI2)}},
