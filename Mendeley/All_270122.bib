keywords = {MST,Microscopy,SNARE,STED,V H H,antibody-antigen,nanobodies,nanobody,neurology,sdAb,super-resolution},
mendeley-tags = {MST,antibody-antigen,nanobodies,neurology},
number = {2},
pages = {305--321},
publisher = {Taylor & Francis},
title = {{Nanobodies reveal an extra-synaptic population of SNAP-25 and Syntaxin 1A in hippocampal neurons}},
